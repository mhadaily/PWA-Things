{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "gulp serve",
    "deploy": "netlify deploy --prod",
    "build": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve -P https --hostname 0.0.0.0 --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.1.0",
    "@polymer/polymer": "^3.3.0",
    "@types/esc-pos-encoder": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "esc-pos-encoder": "^1.2.0",
    "lit-element": "^2.2.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.3.2",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^10.2.0",
    "netlify-cli": "^2.17.0",
    "pixelmatch": "^5.0.2",
    "polymer-cli": "^1.9.11",
    "polyserve": "^0.27.15",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.19.0",
    "typescript": "^3.6.2",
    "wct-mocha": "^1.0.1"
  }
}
